@model IReadOnlyCollection<BookModel>

@{
    ViewData["Title"] = "Результаты поиска";
}

<h1>Результаты поиска</h1>
@if (Model.Count == 0)
{
    <p>Ничего не найдено</p>
}
else
{
    <ul>
        @foreach (var book in Model)
        {
            <li><a asp-controller="Book" asp-action="Index" asp-route-id="@book.Id">@book.Title</a></li>
        }
    </ul>
}

